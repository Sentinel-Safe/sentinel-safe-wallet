# Kaia Kairos Testnet Configuration
# Copy this file to .env and fill in your values

# Network Configuration (Kairos Testnet ONLY)
KAIROS_RPC_URL=https://public-en.kairos.node.kaia.io

# Deployment Private Key (TESTNET ONLY - Never use mainnet keys!)
DEPLOYER_PRIVATE_KEY=0x...

# Safe Owner Addresses (5 total: 2 humans + 3 AI)
# Human Signers
HUMAN_SIGNER_1=0x...
HUMAN_SIGNER_2=0x...

# AI Agent Addresses (generated from backend services)
AI_CFO_ADDRESS=0x...
AI_SECURITY_ADDRESS=0x...
AI_ANALYST_ADDRESS=0x...

# Safe Configuration
SAFE_THRESHOLD=4  # 4-of-5 signatures required

# Optional: Block Explorer API Key for contract verification
ETHERSCAN_API_KEY=

# After deployment, these will be populated:
# SAFE_PROXY_ADDRESS=0x...
# SAFE_ROLE_GUARD_ADDRESS=0x...